let contador = "0"
let registroInstrucciones = "0"
function decode(instruccion){
    const opCode = instruccion.slice(0, 4);
    console.log(opCode)
    if(opCode == "0000"){
        print("suma")
    }
    if(opCode == "0011"){
        print("potencia")
    }
    if(opCode == "0111"){
        print("finalizar")
    }
    if(opCode == "0110"){
        print("guardar")
    }
}
export { decode};